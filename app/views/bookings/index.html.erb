<div class="all-bookings">
  <h1><strong>Upcoming Trips</strong> ðŸŽ‰ </h1>
  <% @bookings.each do |booking| %>
    <div class="card-with-info">
      <img class="family-photo" src= <%= booking.homestay.img_url %>/>
      <div class="info-about-family">
        <h2><%= booking.user.last_name %> Family</h2>
        <p><strong><%= booking.homestay.address %></strong></p>
        <p><%= booking.booking_start %> - <%= booking.booking_end%></p>
      </div>
      <div class="card-status">
        <%=booking.status %>
      </div>
    </div>
  <% end %>
</div>
